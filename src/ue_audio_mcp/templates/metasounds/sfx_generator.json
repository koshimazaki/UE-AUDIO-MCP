{
  "name": "SFX Generator",
  "asset_type": "Source",
  "author": "Eric Buchholz (TechAudioTools, Epic Games)",
  "description": "Single-generator synthesizer for rapid SFX iteration from TechAudioTools. Signal flow: Switchable Waveform Generator (5 types) with frequency modulation \u2192 Spectral Effects (Wave Shaper \u2192 BitCrusher \u2192 Ring Modulator) \u2192 State Variable Filter (crossfade LP/BP/HP with envelope modulation) \u2192 Amplifier (ADSR \u00d7 LFO) \u2192 Temporal Effects (Delay + Flanger + Plate Reverb in parallel) \u2192 Output + Wave Writer + Envelope Follower. Designed for sci-fi and abstract sounds with full randomization support.",
  "source_url": "https://dev.epicgames.com/community/learning/tutorials/ZMpq/unreal-engine-tech-audio-tools-content-documentation",
  "requires": [
    "TechAudioTools engine plugin enabled",
    "UMG Viewmodel engine plugin enabled"
  ],
  "interfaces": [
    "UE.Source.OneShot"
  ],
  "inputs": [
    {
      "name": "OnPlay",
      "type": "Trigger"
    },
    {
      "name": "BaseFrequency",
      "type": "Float",
      "default": 440.0,
      "description": "Base oscillator frequency in Hz (mapped from normalized input via log scale)"
    },
    {
      "name": "FreqMod",
      "type": "Float",
      "default": 0.0,
      "description": "Pitch slide amount \u2014 summed with vibrato for frequency modulation"
    },
    {
      "name": "VibratoRate",
      "type": "Float",
      "default": 5.0,
      "description": "Vibrato LFO rate \u2014 up to audio rate for FM synthesis"
    },
    {
      "name": "VibratoDepth",
      "type": "Float",
      "default": 0.0,
      "description": "Vibrato depth \u2014 0 = off, higher = wider pitch wobble"
    },
    {
      "name": "WaveShapeAmount",
      "type": "Float",
      "default": 0.0,
      "description": "Wave shaper drive \u2014 0 = clean, 1 = soft clip / rounded pulse"
    },
    {
      "name": "CrushSampleRate",
      "type": "Float",
      "default": 48000.0,
      "description": "BitCrusher sample rate reduction \u2014 lower = more aliased"
    },
    {
